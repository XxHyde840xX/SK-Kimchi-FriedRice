<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>RawNapaCabbage</defName>
    <label>Napa Cabbage</label>
    <description>Korean cabbage, born through breeding by Dr. Woo Jang-chun.</description>
    <possessionCount>10</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/NapaCabbage</texPath>
    </graphicData>
    <statBases>
      <MarketValue>10.0</MarketValue>
    </statBases>
    <ingestible>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

  <ThingDef ParentName="PlantFoodRawBase">
    <defName>RawPepper</defName>
    <label>Gochu Pepper</label>
    <description>A plant of the Solanaceae family with a sharp, spicy taste. If you dry and grind ripe red peppers, you can make 'gochu powder', a representative Korean spice.</description>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <possessionCount>10</possessionCount>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/Pepper</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.1</MarketValue>
    </statBases>
    <ingestible>
      <foodType>VegetableOrFruit</foodType>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>60</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>
  
</Defs>
